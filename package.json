{
  "name": "@outloud/css",
  "version": "1.0.1",
  "author": "Outloud <hello@outloud.co> (https://outloud.co)",
  "contributors": [
    "Andrej Adamcik <andrej@outloud.co>"
  ],
  "description": "",
  "keywords": [
    "unocss"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./reset.css": "./dist/reset.css",
    "./helpers.scss": "./dist/helpers.scss"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepack": "pnpm build",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "play": "pnpm --filter ./playground run dev",
    "lint": "eslint .",
    "format": "prettier --write .",
    "release": "pnpm prepack && release-it",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@unocss/core": ">= 0.52.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/node": "^20.3.1",
    "@unocss/core": "^0.53.4",
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "release-it": "^15.11.0",
    "sass": "^1.63.6",
    "typescript": "^5.0.2",
    "unbuild": "^1.2.1"
  },
  "dependencies": {
    "@unocss/preset-mini": "^0.53.3"
  },
  "publishConfig": {
    "@outloud:registry": "https://git.outloud.dev/api/v4/projects/228/packages/npm/"
  }
}
