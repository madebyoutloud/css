{
  "name": "@outloud/css",
  "type": "module",
  "version": "3.0.1",
  "description": "CSS preset for UnoCSS by Outloud.",
  "license": "MIT",
  "homepage": "https://css.byoutloud.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/madebyoutloud/css.git"
  },
  "author": "Outloud <hello@outloud.co> (https://outloud.co)",
  "contributors": [
    "Andrej Adamcik <andrej@outloud.co>"
  ],
  "keywords": [
    "unocss",
    "outloud",
    "css"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./helpers": {
      "types": "./dist/helpers.d.mts",
      "default": "./dist/helpers.mjs"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "play": "pnpm --filter ./playground run dev",
    "lint": "eslint .",
    "release": "pnpm build && release-it",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@unocss/core": "^66",
    "@unocss/preset-mini": "^66",
    "@unocss/reset": "^66.5.11",
    "@unocss/rule-utils": "^66"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@outloud/eslint-config": "^2.0.7",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.1.0",
    "release-it": "^19.2.2",
    "sass-embedded": "^1.97.1",
    "tsdown": "^0.18.3",
    "typescript": "~5.8.3"
  }
}
